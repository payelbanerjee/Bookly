header.page-head
  // RD Navbar
  .rd-navbar-wrap
    nav.rd-navbar(data-layout='rd-navbar-fixed', data-sm-layout='rd-navbar-fixed', data-sm-device-layout='rd-navbar-fixed', data-md-device-layout='rd-navbar-fixed', data-md-layout='rd-navbar-fullwidth', data-lg-layout='rd-navbar-static', data-lg-device-layout="rd-navbar-static", data-sm-stick-up-offset="50px", data-lg-stick-up-offset="90px")
      .rd-navbar-inner
        // RD Navbar Panel
        .rd-navbar-panel
          // RD Navbar Toggle
          button.rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar-nav-wrap')
            span
          // RD Navbar Brand
          .rd-navbar-brand
            a.brand-name(href='index.html')
              img(src="images/logo.png" width="162" height="61" alt="")

        .rd-navbar-nav-wrap
          include ../_navigation
